{
  "name": "@goondan/cli",
  "version": "0.0.1",
  "description": "Goondan - Agent Swarm Orchestrator CLI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "gdn": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./commands": {
      "types": "./dist/commands/index.d.ts",
      "import": "./dist/commands/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@goondan/core": "workspace:*",
    "ai": "^4.3.0",
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "commander": "^13.1.0",
    "chalk": "^5.4.2",
    "ora": "^8.1.2",
    "prompts": "^2.4.2",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "agent",
    "swarm",
    "orchestrator",
    "llm",
    "ai",
    "cli"
  ],
  "license": "MIT"
}
