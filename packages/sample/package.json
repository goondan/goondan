{
  "name": "@goondan/sample",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "bundle:add": "node ../core/dist/cli/index.js bundle add ../base/bundle.yaml --state-root ./state",
    "bundle:enable": "node ../core/dist/cli/index.js bundle enable base --state-root ./state",
    "bundle:disable": "node ../core/dist/cli/index.js bundle disable base --state-root ./state",
    "bundle:info": "node ../core/dist/cli/index.js bundle info base --state-root ./state",
    "bundle:validate": "node ../core/dist/cli/index.js bundle validate base --state-root ./state",
    "bundle:verify": "node ../core/dist/cli/index.js bundle verify base --state-root ./state",
    "bundle:refresh": "node ../core/dist/cli/index.js bundle refresh base --state-root ./state",
    "bundle:lock": "node ../core/dist/cli/index.js bundle lock --state-root ./state",
    "bundle:verify-lock": "node ../core/dist/cli/index.js bundle verify-lock --state-root ./state",
    "bundle:list": "node ../core/dist/cli/index.js bundle list --state-root ./state",
    "bundle:remove": "node ../core/dist/cli/index.js bundle remove base --state-root ./state",
    "export": "node ../core/dist/cli/index.js export -c ./goondan.yaml -b ../base/bundle.yaml --format yaml",
    "validate:strict": "node ../core/dist/cli/index.js validate -c ./goondan.yaml -b ../base/bundle.yaml --strict",
    "run": "node ../core/dist/cli/index.js run -c ./goondan.yaml -b ../base/bundle.yaml --mock --input \"hello\"",
    "run:registered": "node ../core/dist/cli/index.js run -c ./goondan.yaml --state-root ./state --mock --input \"hello\""
  }
}
